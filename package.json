{
  "name": "ml-frontend",
  "version": "0.1.0",
  "author": "Federico Fern√°ndez",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "dotenv": "4.0.0",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "object-assign": "4.1.1",
    "promise": "7.1.1",
    "react": "^15.6.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar app/src/ -o app/src/",
    "watch-css": "npm run build-css && node-sass-chokidar app/src/ -o app/src/ --watch --recursive",
    "start-js": "node app/scripts/start.js",
    "start-server": "nodemon server.js",
    "start-server-prod": "NODE_ENV=production PORT=3000 node server.js",
    "start": "npm-run-all -p watch-css start-js start-server",
    "build": "npm run build-css && node app/scripts/build.js",
    "prod": "npm run build && npm run start-server-prod"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "nodemon": "^1.11.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0"
  }
}
